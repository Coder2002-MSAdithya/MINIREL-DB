select into BigSpenders from Users where ( Balance >= 1000.00 );
print BigSpenders;
project into UserContacts from Users ( Name, Email );
print UserContacts;
project into UserAges from Users ( UserId, Age );
print UserAges;
join into UserOrders ( Users.UserId, Orders.UserId );
print UserOrders;
join into OrderWithItems ( Orders.OrderId, OrderItems.OrderId );
print OrderWithItems;
join into ProductInventory ( Products.ProductId, Inventory.ProductId );
print ProductInventory;
select into HighValueOrders from Orders where ( Total > 5000.00 );
print HighValueOrders;
project into OrderSummaries from Orders ( OrderId, Total );
print OrderSummaries;
select into Electronics from Products where ( Category = "Electronics" );
print Electronics;
join into ElectronicsStock ( Electronics.ProductId, Inventory.ProductId );
print ElectronicsStock;
delete from Products where ( Stock < 20 );
print Products;
delete from Users where ( UserId = 3 );
print Users;
delete from OrderItems where ( Quantity > 4 );
print OrderItems;
insert into Users ( UserId = 1, Name = "Alice", Email = "[alice@example.com](mailto:alice@example.com)", Age = 28, Balance = 1250.50 );
select into YoungUsers from Users where ( Age <= 25 );
print YoungUsers;
project into CompactProducts from Products ( ProductId, Name, Price );
print CompactProducts;
join into UserPayments ( Users.UserId, Payments.OrderId );
print UserPayments;
select into PendingPayments from Payments where ( Status = "PENDING" );
print PendingPayments;
delete from Payments where ( Status = "FAILED" );
print Payments;
insert into Inventory ( ProductId = 13, SupplierId = 3, Qty = 5, Warehouse = "W1" );
