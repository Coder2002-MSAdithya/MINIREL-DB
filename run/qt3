print Products;
join into ProductSuppliers ( Products.ProductId, Suppliers.SupplierId );
print ProductSuppliers;
project into UserBalances from Users ( UserId, Balance );
print UserBalances;
select into LowStock from Inventory where ( Qty < 15 );
print LowStock;
delete from Inventory where ( ProductId = 9 );
print Inventory;
