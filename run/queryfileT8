print relcat;
print attrcat;
create Student(ID=i, Name=s16, Dept=s16, GPA=f);
create Professor(Faculty=s16, Dept=s16, SID=i);
create Course(Course=s16, Credits=i, Dept=s16);
create Enrollment(SID=i, Course=s16, Semester=s16);
print relcat;
print attrcat;
print Student;
print Professor;
print Course;
print Enrollment;
