join into StudentEnrollment (Student.ID, Enrollment.SID);
join into CourseEnrollment (Enrollment.Course, Course.Course);
join into AdvisorMappingPossibility (Student.Dept, Professor.Dept);
join into CourseMappingPossibility (Student.Dept, Course.Dept);
join into StudentAdvisors (Student.ID, Professor.SID);
